{
  "name": "gplus-unlocker",
  "version": "1.0.2",
  "description": "Gplus Unlocker is a webextensions that unlocks the premium articles on Gazzetta.it",
  "main": "src",
  "webExt": {
    "sourceDir": "src/"
  },
  "scripts": {
    "firefox:test": "web-ext run -s=./src --start-url https://www.gazzetta.it/premium/ --firefox=firefoxdeveloperedition",
    "firefox:build": "web-ext build  -a=./build",
    "firefox:lint": "web-ext lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salsan/gplus-unlocker.git"
  },
  "keywords": [
    "paywall-unlocker",
    "paywall-blocker",
    "paywall-bypass"
  ],
  "author": "Salvatore Santagati",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/salsan/gplus-unlocker/issues"
  },
  "homepage": "https://github.com/salsan/gplus-unlocker#readme",
  "devDependencies": {
    "web-ext": "^8.0.0"
  }
}